{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "angular",
        "printWidth": 90
      }
    },
    {
      "files": "*.ts",
      "options": {
        "printWidth": 80
      }
    }
  ]
}
